{
  "name": "metronome-desktop-wallet",
  "version": "0.2.0",
  "private": true,
  "author": {
    "name": "Bloq Labs",
    "email": "labs@bloq.com",
    "url": "http://bloq.com/"
  },
  "description": "Metronome Wallet for desktop",
  "repository": {
    "type": "git",
    "url": "git@github.com:MetronomeToken/metronome-desktop-wallet.git"
  },
  "dependencies": {
    "7zip-bin": "^3.1.0",
    "axios": "^0.17.1",
    "bignumber.js": "^6.0.0",
    "bip39": "^2.5.0",
    "coincap-lib": "^1.0.0",
    "cross-env": "^5.1.3",
    "cuid": "^2.0.2",
    "debug": "^3.1.0",
    "electron-debug": "^1.5.0",
    "electron-log": "^2.2.14",
    "electron-settings": "^3.1.4",
    "electron-unhandled": "^1.0.0",
    "electron-updater": "^2.20.1",
    "ethereumjs-tx": "^1.3.3",
    "ethereumjs-util": "^5.1.3",
    "ethereumjs-wallet": "^0.6.0",
    "human-standard-token-abi": "^1.0.2",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "moment-precise-range-plugin": "^1.3.0",
    "nedb": "^1.8.0",
    "node-notifier": "^5.2.1",
    "promise-all-props": "^1.0.1",
    "qrcode.react": "^0.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-modal": "^3.1.11",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-toastify": "^3.3.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "reselect": "^3.0.1",
    "safe-buffer": "^5.1.1",
    "socket.io-client": "^2.0.4",
    "styled-components": "^3.0.2",
    "web3": "^1.0.0-beta.29"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "copyright": "Copyright Â© 2018 ${author}",
  "build": {
    "appId": "com.bloqlabs.metronome",
    "productName": "Metronome-Wallet",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github"
    },
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "com.bloqlabs.metronome.category.finance"
    },
    "dmg": {
      "backgroundColor": "#7e61f8",
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "./assets/icon",
      "legalTrademarks": ""
    }
  },
  "config": {
    "cra_dev_serv": "http://localhost:3000"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier --write ./**/*.js",
    "pack": "react-scripts build && builder --dir",
    "dev": "concurrently \"BROWSER=none npm start\" \"wait-on $npm_package_config_cra_dev_serv && ELECTRON_START_URL=$npm_package_config_cra_dev_serv electron .\"",
    "debug": "concurrently \"BROWSER=none npm start\" \"wait-on $npm_package_config_cra_dev_serv && ELECTRON_START_URL=$npm_package_config_cra_dev_serv electron . --inspect\"",
    "dist": "react-scripts build && build --em.main=build/electron.js",
    "release": "react-scripts build && build --em.main=build/electron.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "devtron": "^1.4.0",
    "electron": "^1.7.11",
    "electron-builder": "^19.56.0",
    "electron-devtools-installer": "^2.2.3",
    "electron-is-dev": "^0.3.0",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^1.10.2",
    "proxyquire": "^1.8.0",
    "wait-on": "^2.1.0"
  }
}
